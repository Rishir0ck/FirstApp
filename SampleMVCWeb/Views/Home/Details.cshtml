@{
    Layout = null;
    var student = ViewBag.Student;
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.title</title>
</head>
<body>
    <h1>@ViewBag.Header</h1>
<div>
    Student ID: @student?.StudentId
</div>
<div>
    Name: @student?.Name
</div>
<div>
    Branch: @student?.Branch
</div>
<div>
    Section: @student?.Section
</div>
<div>
    Gender: @student?.Gender
</div>

</body>